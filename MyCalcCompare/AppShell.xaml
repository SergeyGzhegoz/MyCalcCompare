<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyCalcCompare.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyCalcCompare"
    Shell.FlyoutBehavior="Disabled">
    <TabBar>
    <ShellContent
        Title="Calc"
        ContentTemplate="{DataTemplate local:MainPage}"
        Icon="calculator" />

    <ShellContent
        Title="Compare"
        ContentTemplate="{DataTemplate local:Compare}"
        Icon="deciion" />
    </TabBar>
</Shell>
